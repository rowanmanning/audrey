{
  "name": "@rowanmanning/audrey",
  "version": "2.0.0-beta.10",
  "private": true,
  "description": "A simple single-user feed reader",
  "keywords": [
    "atom",
    "feed",
    "reader",
    "rss"
  ],
  "author": "Rowan Manning (https://rowanmanning.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowanmanning/audrey.git"
  },
  "homepage": "https://github.com/rowanmanning/audrey",
  "bugs": "https://github.com/rowanmanning/audrey/issues",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=14",
    "npm": ">=7",
    "mongodb": "4 || 5"
  },
  "scripts": {
    "start": "node .",
    "start:dev": "nodemon .",
    "verify": "eslint .",
    "test": "npm run test:integration",
    "test:integration": "mocha 'test/integration/**/*.test.js'",
    "test:feeds": "mocha 'test/feeds/**/*.test.js' --timeout 50000 --slow 10000",
    "project:verify": "npx --yes @rowanmanning/validate-project --type git node-express-app",
    "project:fix": "npx --yes @rowanmanning/validate-project --type git node-express-app --fix"
  },
  "dependencies": {
    "@rowanmanning/app": "^1.0.0-alpha.25",
    "@rowanmanning/fetch-feed": "^1.0.1",
    "@rowanmanning/response-redirect-middleware": "^1.0.2",
    "@rowanmanning/response-render-middleware": "^1.0.2",
    "bcrypt": "^5.0.1",
    "date-fns": "^2.28.0",
    "dompurify": "^2.3.6",
    "dotenv": "^16.0.0",
    "got": "^11.8.3",
    "jsdom": "^19.0.0",
    "mongoose-unique-validator": "^2.0.3",
    "netscape-bookmarks": "^0.1.1",
    "node-schedule": "^2.1.0",
    "shortid": "^2.2.16",
    "srcset": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rowanmanning/eslint-config": "^3.3.0",
    "@rowanmanning/require-all": "^2.0.1",
    "chai": "^4.3.6",
    "eslint": "^8.10.0",
    "mocha": "^9.2.1",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "testdouble": "^3.16.4"
  },
  "mocha": {
    "timeout": 10000
  },
  "nyc": {
    "include": [
      "server/**/*.js",
      "server/**/*.jsx"
    ]
  },
  "nodemonConfig": {
    "ext": "js,jsx",
    "ignore": [
      "test/*"
    ]
  }
}
